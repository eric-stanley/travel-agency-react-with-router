{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build/client/assets"
      }
    },
    {
      "src": "build/server/index.js",
      "use": "@vercel/node",
      "config": {
        "distDir": "build/server"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ]
}
